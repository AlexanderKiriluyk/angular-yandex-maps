{
  "extends": "tslint-config-airbnb",
  "rules": {
    "variable-name": {
      "options": [
        "allow-leading-underscore"
      ]
    },
    "function-name": [false],
    "trailing-comma": [false],
    "prefer-array-literal": [true, { "allow-type-parameters": true }],
    "max-line-length": [true, 140],
    "no-increment-decrement": false
  }
}